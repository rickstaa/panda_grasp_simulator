{
  "name": "openai-ros-ws",
  "version": "1.1.3",
  "description": "Workspace package for the openai_ros and openai_examples_projects.",
  "keywords": [
    "reinforcement",
    "learning",
    "ros",
    "openai_ros",
    "robotics",
    "openai-gym",
    "environments"
  ],
  "homepage": "https://github.com/rickstaa/openai_ros_ws#readme",
  "bugs": {
    "url": "https://github.com/rickstaa/openai_ros_ws/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rickstaa/openai_ros_ws.git"
  },
  "license": "UNLICENSED",
  "author": "Rick Staa",
  "main": "index.js",
  "scripts": {
    "changelog": "HUSKY=0 standard-version --skip.bump --skip.commit --skip.tag",
    "prepare": "husky install",
    "release": "HUSKY=0 standard-version"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "!(CHANGELOG)*.md": "./node_modules/.bin/remark . -o --"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "15.0.0",
    "@commitlint/config-conventional": "15.0.0",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "husky": "7.0.4",
    "lint-staged": "12.0.1",
    "remark": "14.0.2",
    "remark-cli": "10.0.0",
    "remark-lint": "9.1.0",
    "remark-preset-lint-recommended": "6.1.1",
    "standard-version": "9.3.2"
  }
}
